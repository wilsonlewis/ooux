{
  "name": "simple",
  "description": "Elephant office meeting room calendar.",
  "version": "1.0.0",
  "author": "Wilson Lewis <wlewis@helloelephent.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "bundle": "webpack --progress --hide-modules --config webpack/index",
    "bundle:serve": "webpack-dev-server --hot --config webpack/index",
    "serve": "node server/index"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false,
          "targets": {
            "browsers": [
              "last 2 versions",
              "safari >= 7"
            ]
          }
        }
      ],
      "stage-3"
    ]
  },
  "_moduleAliases": {
    "App": "app",
    "Views": "assets/views",
    "Styles": "assets/styles",
    "Elephant": "elephant",
    "assets": "assets"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8",
    "safari >= 7"
  ],
  "dependencies": {
    "autosize": "^4.0.0",
    "axios": "^0.17.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "body-parser": "^1.18.2",
    "collect.js": "^4.0.2",
    "contentful": "^5.0.5",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.5.4",
    "eases": "^1.0.8",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "express-spa": "0.0.2",
    "file-loader": "^1.1.4",
    "googleapis": "^23.0.0",
    "html-webpack-plugin": "^2.30.1",
    "lodash": "^4.17.4",
    "module-alias": "^2.0.3",
    "moment": "^2.20.1",
    "node-sass": "^4.5.3",
    "normalize.css": "^7.0.0",
    "passport": "^0.4.0",
    "passport-google-oauth": "^1.0.0",
    "prettier": "^1.10.2",
    "progressbar.js": "^1.0.1",
    "pug": "^2.0.0-rc.4",
    "pug-loader": "^2.3.0",
    "sass-loader": "^6.0.6",
    "vue": "^2.5.11",
    "vue-flickity": "^1.0.9",
    "vue-loader": "^13.0.5",
    "vue-markdown": "^2.2.4",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.4.4",
    "vue-touch": "^2.0.0-beta.4",
    "vuex": "^3.0.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-manifest-plugin": "^1.3.2"
  }
